<script type="text/javascript">

    function renderTime(date)
    {
        var da = new Date(parseInt(date.replace("/Date(", "").replace(")/", "").split("+")[0]));
        return da.getFullYear() + "-" 
            + (da.getMonth() + 1) + "-" 
            + da.getDate() + " " 
            + da.getHours() + ":" 
            + da.getMinutes() + ":" 
            + da.getSeconds();
    }

    var _turnover={
        GetData:function(){},
        InseToTable:function(data,tbody){  //把数据插入到表格中

            $.each(data,function(){
                var row=$("#TempTurnover tr").clone();

                row.find("[name=Identifier]").text(this.Identifier);
                row.find("[name=UserName]").text(this.UserName);
                row.find("[name=SkuName]").text(this.SkuName);
                row.find("[name=Quantity]").text(this.Quantity);
                row.find("[name=Price]").text(this.Price);

                var str= renderTime(this.CreateTime);
                row.find("[name=CreateTime]").text(str);

                row.appendTo(tbody);
            });
        }
    };

    _turnover.GetData=function(turnoverType,trueFun,falseFun )
    {
        var index = 0;
        $.ajax({
            url: "/ForeJson/TurnoverGetList",
            data: {
                goodsid: _view.model.Identifier ,
                turnoverType:turnoverType
            },
            dataType: "json",
            type: "post",
            cache: false,
            beforeSend: function () { index =layer.load({shade:0});},
            complete: function () { layer.close(index); },            
            error: function () {  },
            success: function (r)
            {
                if (r && r.success)
                {
                    trueFun(r)  //调用回调函数，参数返回数据
                }
                else
                {
                    if(falseFun)  //如果有失败函数，就执行，没有就执行默认的。 
                    {
                        falseFun(r); //调用回调函数，参数返回数据
                    }
                }
            }
        });

    }


    _view.TabClick=function(_this)
    {
        var $li=$(_this);
        var index=$li.attr("index")-0; //索引值

        //设置样式
        $li.addClass("hover");  
        $li.siblings().each(function(){
            $(this).removeClass("hover");
        });

        //显示内容
        $("[name=con_TabLi]").each(function(){
            if($(this).attr("index")==index)
            {
                $(this).show();
            }
            else
            {
                $(this).hide();
            }
        });
        //填充月成交量
        if(index==3)
        {
            var tbody=$("[name=con_TabLi][index=3] tbody");
            tbody.empty();
            _turnover.GetData("Month",function(r)
            {
                _turnover.InseToTable(r.data,tbody);
            });
        }
        else if (index==4) //填充总成交量
        {
            var tbody=$("[name=con_TabLi][index=4] tbody");
            tbody.empty();

            _turnover.GetData("All",function(r){
                _turnover.InseToTable(r.data,tbody);
            });
        }
    }

    function setTab(name, cursel, n)
    {
        for (i = 1; i <= n; i++)
        {
            var menu = document.getElementById(name + i);
            var con = document.getElementById("con_" + name + "_" + i);
            menu.className = i == cursel ? "hover" : "";
            con.style.display = i == cursel ? "block" : "none";
        }
    };

</script>

