                   });
               }
           }
       };
        
       _ucImgMagnifier.ShowArrow = function ()
       {
           var sum = _ucImgMagnifier.ThumbUl.find("li").length; //总数量
           if (sum > 0)  //有图片，没有就不用计算了
           {
               var index = _ucImgMagnifier.ThumbUl.find("li:visible").first().attr("index") - 0;

               if (index == 1)
               {
                   _ucImgMagnifier.ThumbRight.hide();
               }
               else
               {
                   _ucImgMagnifier.ThumbRight.show();
               }

               if ((index + 5) >= sum)
               {
                   _ucImgMagnifier.ThumbLeft.hide();
               }
               else
               {
                   _ucImgMagnifier.ThumbLeft.show();
               }
           }
       };

